{
    "metadata": {
        "name": "Describo Test Profile with datapacks and without groups",
        "description": "A profile with entries for each of the supported datatypes",
        "version": 0.1,
        "warnMissingProperty": true
    },
    "classes": {
        "Dataset": {
            "definition": "override",
            "subClassOf": [],
            "inputs": [
                {
                    "id": "https://schema.org/language",
                    "name": "language",
                    "label": "Language",
                    "help": "Attach a language",
                    "type": ["Language"],
                    "required": true,
                    "multiple": false
                },
                {
                    "id": "https://schema.org/also",
                    "name": "also",
                    "help": "Attach a language",
                    "type": ["Language"],
                    "required": true,
                    "multiple": false
                },
                {
                    "id": "http://schema.org/contentLocation",
                    "name": "location",
                    "help": "Location [min/max lat/long]. Given by bounding box on map.",
                    "type": ["Geo"]
                },
                {
                    "id": "http://schema.org/author",
                    "name": "author",
                    "help": "Location [min/max lat/long]. Given by bounding box on map.",
                    "type": ["Person"]
                }
            ]
        },
        "Person": {
            "definition": "override",
            "subClassOf": [],
            "inputs": [
                {
                    "id": "https://schema.org/name",
                    "name": "name",
                    "label": "name",
                    "help": "The name the person",
                    "required": true,
                    "multiple": false,
                    "type": ["Text"]
                }
            ]
        },
        "Organisation": {
            "definition": "override",
            "subClassOf": [],
            "inputs": [
                {
                    "id": "https://schema.org/name",
                    "name": "name",
                    "label": "name",
                    "help": "The name of the organisation",
                    "required": true,
                    "multiple": false,
                    "type": ["Text"]
                }
            ]
        },
        "Language": {
            "definition": "override"
        }
    },
    "lookup": {
        "Language": {
            "fields": [
                "@id",
                "name",
                "alternateName",
                "iso639-3",
                "austlangCode",
                "glottologCode",
                "languageCode"
            ]
        }
    }
}
